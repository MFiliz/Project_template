    <script>
       

        function StartLoading() {
            $(".loading").show();
        }

        function StopLoading() {
            $(".loading").hide();
        }

        function MyLoader() {

            return '<div class="cssload-container"><div class="cssload-lt"></div><div class="cssload-rt"></div><div class="cssload-lb"></div><div class="cssload-rb"></div></div>';

        }

        function MyDatePickerFields() {
            $(".mydatepicker").daterangepicker({
